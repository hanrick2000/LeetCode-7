lower= 1 upper = 2

lower =1000, upper = 2000 upper limit->9
// Count nos from 1...n, in which numbers of digits are between [lower, upper]
int count(int n, int lower, int upper){
        //validation  
        if(n<0 || lower>upper || lower<0 || upper>9){. Waral 鍗氬鏈夋洿澶氭枃绔�,
                if(upper>9){. Waral 鍗氬鏈夋洿澶氭枃绔�,
                        System.out.println("invalid input, integer can only be 0-2^32");. 鐣欏鐢宠璁哄潧-涓€浜╀笁鍒嗗湴
                }
                return 0;-google 1point3acres
        }
        //count the length of the digits of n.鐣欏璁哄潧-涓€浜�-涓夊垎鍦�
        int length=0;
        while(n>0){ 鏉ユ簮涓€浜�.涓夊垎鍦拌鍧�. 
                n/=10;
                length++;
        }
        //length,upper,lower. 鐣欏鐢宠璁哄潧-涓€浜╀笁鍒嗗湴
        if(length<lower){
                return 0;
        }else if(length<=upper){//n=999 lower=2,upper=3 10-299 
                return n-(Math.pow(10,lower-1)-1);
        }else{//length>upper n=1m,lower=2,upper=3 10-99
                return (Math.pow(10,upper)-1)-(Math.pow(10,lower-1)-1);
        }
}

